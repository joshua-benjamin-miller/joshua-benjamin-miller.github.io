      },
      {
        "type": "expression",
        "id": "748",
        "folderId": "504",
        "color": "#6042a6",
        "latex": "B\\left(Q\\right)=a_{B}+b_{B}Q-c_{B}Q^{2}\\left\\{0\\le Q\\right\\}",
        "hidden": true
      },
      {
        "type": "expression",
        "id": "505",
        "folderId": "504",
        "color": "#6042a6",
        "latex": "a_{B}=0",
