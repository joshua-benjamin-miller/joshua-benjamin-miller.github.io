        "type": "expression",
        "id": "758",
        "folderId": "688",
        "color": "#388c46",
        "latex": "U_{type}=1",
        "slider": {
          "hardMin": true,
          "hardMax": true,
          "min": "1",
          "max": "2",
          "step": "1"
        }
      },
      {
        "type": "expression",
        "id": "770",
        "folderId": "688",
        "color": "#c74440",
        "latex": "\\left(-3,3\\right)",
        "showLabel": true,
        "label": "Normal Good",
        "pointStyle": "OPEN",
        "clickableInfo": {
          "enabled": true,
          "latex": "U_{type}\\to1"
        }
      },
      {
        "type": "expression",
        "id": "771",
        "folderId": "688",
        "color": "#2d70b3",
        "latex": "\\left(-3,2\\right)",
        "showLabel": true,
        "label": "Inferior Good",
        "pointStyle": "OPEN",
        "clickableInfo": {
          "enabled": true,
          "latex": "U_{type}\\to2"
        }
      },
      {
        "type": "expression",
        "id": "761",
        "folderId": "688",
        "color": "#c74440",
        "latex": "C_{hoice}\\left(f_{1},f_{2}\\right)=\\left\\{U_{type}=1:f_{1},\\ U_{type}=2:f_{2}\\right\\}"
      },
      {
        "type": "expression",
        "id": "756",
        "folderId": "688",
        "color": "#c74440",
        "latex": "u\\left(x,y\\right)=C_{hoice}\\left(v_{1}\\left(x,y\\right),v_{2}\\left(x,y\\right)\\right)"
      },
      {
        "type": "expression",
        "id": "757",
        "folderId": "688",
        "color": "#2d70b3",
        "latex": "a=1",
        "hidden": true,
        "slider": {
          "hardMin": true,
          "min": "0"
