      {
        "type": "text",
        "id": "76",
        "text": "Demand with Inferior Goods"
      },
      {
        "type": "text",
        "id": "108",
        "text": "Inspired by \"New Thoughts About Inferior Goods\" H. H. Liebhafsky's paper from The American Economic Review:https://www.jstor.org/stable/1810692"
      },
      {
        "type": "folder",
        "id": "77",
        "title": "Credits",
        "hidden": true,
        "collapsed": true
      },
      {
        "type": "text",
        "id": "79",
        "folderId": "77",
        "text": "By: Joshua B. Miller"
      },
      {
        "type": "text",
        "id": "83",
        "folderId": "77",
        "text": "https://www.joshua-benjamin-miller.com/"
      },
      {
        "type": "text",
        "id": "87",
        "folderId": "77",
        "text": " From: Liebhafsky, H. H. (1969). New thoughts about inferior goods. The American Economic Review, 59(5), 931-934."
      },
      {
        "type": "text",
        "id": "82",
        "folderId": "77",
        "text": " https://www.jstor.org/stable/pdf/1810692.pdf"
      },
      {
        "type": "text",
        "id": "91",
        "text": "Budget set"
      },
      {
        "type": "expression",
        "id": "1",
        "color": "#388c46",
        "latex": "p_{1}x+p_{2}y<=I\\left\\{x\\ge0\\right\\}\\left\\{y\\ge0\\right\\}"
      },
      {
        "type": "expression",
        "id": "2",
        "color": "#2d70b3",
        "latex": "p_{1}=1",
        "hidden": true
      },
      {
        "type": "expression",
        "id": "3",
        "color": "#388c46",
        "latex": "p_{2}=0.9",
        "hidden": true
      },
      {
        "type": "expression",
        "id": "4",
        "color": "#6042a6",
        "latex": "I=2.2",
        "hidden": true,
        "slider": {
          "hardMin": true,
          "hardMax": true,
          "min": "0"
        }
      },
      {
        "type": "text",
        "id": "93",
        "text": "Preferences"
      },
      {
        "type": "expression",
        "id": "5",
        "color": "#000000",
        "latex": "u\\left(x,y\\right)=a\\ln\\left(x\\right)+\\frac{y^{2}}{2}"
      },
      {
        "type": "expression",
        "id": "6",
        "color": "#c74440",
        "latex": "a=1.3",
        "hidden": true
      },
      {
        "type": "folder",
        "id": "104",
        "title": "Highest Indifference curve (optimal consumption",
        "collapsed": true
      },
      {
        "type": "text",
        "id": "100",
        "folderId": "104",
        "text": "Demand"
      },
      {
        "type": "text",
        "id": "102",
        "folderId": "104",
        "text": "discriminating factor"
      },
      {
        "type": "expression",
        "id": "10",
        "folderId": "104",
        "color": "#000000",
        "latex": "D=\\left(\\frac{I}{p_{2}}\\right)^{2}-4a"
      },
      {
        "type": "text",
        "id": "14",
        "folderId": "104",
        "text": "corner solution"
      },
      {
        "type": "expression",
        "id": "12",
        "folderId": "104",
        "color": "#2d70b3",
        "latex": "d_{x}=\\frac{I}{p_{1}}"
      },
      {
        "type": "expression",
        "id": "25",
        "folderId": "104",
        "color": "#388c46",
        "latex": "d_{y}=0",
        "slider": {
          "hardMin": true,
          "hardMax": true,
          "min": "0",
          "max": "0"
        }
      },
      {
        "type": "text",
        "id": "16",
        "folderId": "104",
        "text": "Interior solution"
      },
      {
        "type": "expression",
        "id": "17",
        "folderId": "104",
        "color": "#000000",
        "latex": "d_{yi}=\\frac{\\left(\\frac{I}{p_{2}}+D^{0.5}\\right)}{2}"
      },
      {
        "type": "expression",
        "id": "18",
        "folderId": "104",
        "color": "#c74440",
        "latex": "d_{xi}=\\frac{\\left(I-p_{2}d_{yi}\\right)}{p_{1}}"
      },
      {
        "type": "expression",
        "id": "19",
        "folderId": "104",
        "color": "#2d70b3",
        "latex": "u\\left(d_{x},d_{y}\\right)\\left\\{x>0\\right\\}\\left\\{y>0\\right\\}=u\\left(x,y\\right)\\left\\{U_{C}>U_{I},D<0\\right\\}"
      },
      {
        "type": "expression",
        "id": "20",
        "folderId": "104",
        "color": "#fa7e19",
        "latex": "u\\left(d_{xi},d_{yi}\\right)\\left\\{x>0\\right\\}\\left\\{y>0\\right\\}=u\\left(x,y\\right)\\left\\{U_{C}<U_{I}\\right\\}\\left\\{D>0\\right\\}"
      },
      {
        "type": "expression",
        "id": "21",
        "folderId": "104",
        "color": "#6042a6",
        "latex": "U_{C}=u\\left(d_{x},d_{y}\\right)"
      },
      {
        "type": "expression",
        "id": "22",
        "folderId": "104",
        "color": "#000000",
        "latex": "U_{I}=u\\left(d_{xi},d_{yi}\\right)"
      },
      {
        "type": "folder",
        "id": "97",
        "title": "Moveable Indifference curve",
        "hidden": true,
        "collapsed": true
      },
      {
        "type": "text",
        "id": "95",
        "folderId": "97",
        "text": "Moveable Indifference curve"
      },
